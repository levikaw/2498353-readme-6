  "license": "MIT",
  "scripts": {
    "service:file": "npx nx run file:serve",
    "service:gateway": "npx nx run gateway:serve",
    "service:auth": "npx nx run auth:serve",
    "service:post": "npx nx run post:serve",
    "service:user": "npx nx run user:serve",
    "service:artefact": "npx nx run artefact:serve",
    "service:notification": "npx nx run notification:serve",
    "service:postgres": "cd infra/configuration/src/database/postgres && docker compose up -d",
    "service:mongo": "cd infra/configuration/src/database/mongodb && docker compose up -d",
    "service:fakesmtp": "cd infra/configuration/src/service/notification-service && docker compose up -d",
    "service:rabbitmq": "cd infra/configuration/src/rabbitmq && docker compose up -d",
    "lint:all": "npx nx run-many --all --target=lint --fix",
    "prisma:seed": "cd infra/prisma && npx ts-node src/seed-data.ts",
    "prisma:generate": "cd infra/prisma && npx prisma generate",
    "prisma:migration:create": "cd infra/prisma && npx prisma migrate dev --skip-seed",
    "prisma:migration:apply": "cd infra/prisma && npx prisma db push"
  },
  "private": true,
